{
  "name": "yt-man",
  "version": "1.0.0",
  "description": "Extension for time based skips with media keys on YouTube",
  "main": "index.js",
  "scripts": {
    "bundle": "webpack --mode production --config webpack.config.js",
    "watch": "webpack --mode development -w --config webpack.config.dev.js",
    "debug": "web-ext run --args=' --auto-open-devtools-for-tabs' -t chromium --chromium-binary /Applications/Chromium.app/Contents/MacOS/Chromium -s dist/ --reload --start-url 'https://www.youtube.com/watch?v=m5RET4g2qII' --chromium-profile ~/Library/Application\\ Support/Chromium/Default"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "web-ext": "^6.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@types/chrome": "^0.0.142",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/wicg-mediasession": "^1.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
}
