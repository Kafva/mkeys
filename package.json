{
  "name": "mkeys",
  "version": "1.0.0",
  "description": "Browser extension to enable time based skips in YouTube videos using the next-track and previous-track media keys",
  "main": "index.js",
  "scripts": {
    "release": "webpack --mode production --config webpack.config.js",
    "watch": "DEBUG=1 webpack --mode development -w --config webpack.config.js",
    "debug": "web-ext run --args=' --lang en' -t chromium --chromium-binary /Applications/Chromium.app/Contents/MacOS/Chromium -s dist/ --reload --start-url 'https://www.youtube.com/watch?v=9jHwwJgSTR4' --chromium-profile ~/Library/Application\\ Support/Chromium/Default",
    "lint": "eslint . --ext 'ts,tsx,js,jsx'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "web-ext": "^6.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@types/chrome": "^0.0.142",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/wicg-mediasession": "^1.1.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
}
